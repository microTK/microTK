{
  "name": "microTK",
  "version": "v0.0.2-pre",
  "author": "Todd Henderson <todd@todd-henderson.me>",
  "main": "dist/microTK.js",
  "scripts": {
    "build": "./node_modules/coffee-script/bin/cake build",
    "doc": "./node_modules/jsdox/bin/jsdox --templateDir templates dist/microTK.js; cp -f output/microTK.md README.md; rm -rf output/",
    "uglyify": "./node_modules/uglify-js/bin/uglifyjs dist/microTK.js -o dist/microTK.min.js --source-map dist/microTK.min.map -c --preamble '/**  microTK v0.0.2-pre @Copyright (c) Todd Henderson  @license MIT */'",
    "test": "./node_modules/karma/bin/karma start karma.conf.js",
    "test-Local": "./node_modules/karma/bin/karma start karma.local.conf.js",
    "watch": "./node_modules/coffee-script/bin/cake watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:thenderson21/microTK.git"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "install": "^0.8.1",
    "jasmine-core": "^2.4.1",
    "jsdox": "^0.4.10",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1"
  }
}
